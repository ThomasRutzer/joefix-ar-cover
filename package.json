{
  "name": "joe-fix-ar-radio",
  "version": "0.1.0",
  "private": false,
  "homepage": "https://thomasrutzer.dev/joefix-ar-radio/",
  "dependencies": {
    "@react-spring/three": "^9.1.2",
    "@react-three/drei": "4.3.3",
    "@react-three/fiber": "^7.0.0",
    "@types/three": "0.127.1",
    "leva": "^0.9.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-spring": "^9.2.4",
    "react-three-arnft": "github:thomasrutzer/react-three-arnft#03eb550",
    "three": "0.130.0",
    "zustand": "^3.5.1"
  },
  "scripts": {
    "start": "HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d build -b master"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "import/order": [
        2,
        {
          "groups": [
            [
              "builtin",
              "external"
            ],
            [
              "internal",
              "parent",
              "sibling",
              "index"
            ]
          ]
        }
      ],
      "quote-props": [
        2,
        "consistent-as-needed"
      ],
      "quotes": [
        2,
        "double",
        {
          "avoidEscape": true
        }
      ],
      "semi-spacing": 2,
      "semi": [
        2,
        "never"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}